{
  "name": "prettier-plugin-elm",
  "version": "0.12.0",
  "description": "Adds Elm language support to Prettier via elm-format",
  "keywords": [
    "elm",
    "elm-format",
    "gicentre",
    "prettier",
    "prettier-plugin",
    "typescript"
  ],
  "repository": "https://github.com/gicentre/prettier-plugin-elm",
  "license": "BSD-3-Clause",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf cache coverage dist",
    "fix": "npm-run-all2 --continue-on-error \"fix:*\"",
    "fix:eslint": "eslint --fix .",
    "fix:markdownlint": "markdownlint --fix .",
    "fix:pnpm-dedupe": "pnpm dedupe",
    "fix:prettier": "prettier --write .",
    "lint": "npm-run-all2 --continue-on-error \"lint:*\"",
    "lint:eslint": "eslint .",
    "lint:markdownlint": "markdownlint .",
    "lint:pnpm-dedupe": "pnpm dedupe --check",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --noEmit",
    "prepare": "husky",
    "prepublishOnly": "pnpm lint && pnpm test",
    "test": "pnpm clean && pnpm build && pnpm vitest --run"
  },
  "lint-staged": {
    "**": [
      "markdownlint --fix",
      "prettier --write"
    ]
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-packagejson"
    ]
  },
  "dependencies": {
    "elm-format": "0.8.8",
    "execa": "^5.1.1",
    "object-hash": "^3.0.0",
    "serialize-error": "^12.0.0",
    "temp-dir": "^2.0.0"
  },
  "devDependencies": {
    "@kachkaev/eslint-config-base": "0.6.0",
    "@kachkaev/markdownlint-config": "0.5.0",
    "@tsconfig/recommended": "1.0.10",
    "@tsconfig/strictest": "2.0.5",
    "@types/node": "24.5.2",
    "@types/object-hash": "3.0.6",
    "@types/prettier-v1": "npm:@types/prettier@1.19.1",
    "@types/prettier-v2": "npm:@types/prettier@2.7.3",
    "@vitest/coverage-v8": "4.0.8",
    "eslint": "8.57.1",
    "husky": "9.1.7",
    "lint-staged": "16.1.6",
    "markdownlint-cli": "0.45.0",
    "npm-run-all2": "8.0.4",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.19",
    "prettier-v1": "npm:prettier@1.19.1",
    "prettier-v2": "npm:prettier@2.8.8",
    "rimraf": "6.0.1",
    "sleep-promise": "9.1.0",
    "typescript": "5.9.2",
    "vitest": "4.0.8"
  },
  "packageManager": "pnpm@10.17.0",
  "engines": {
    "node": ">=18.20.6"
  }
}
