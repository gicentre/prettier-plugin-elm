{
  "name": "prettier-plugin-elm",
  "version": "0.7.0",
  "description": "Adds Elm language support to Prettier via elm-format",
  "keywords": [
    "elm",
    "elm-format",
    "gicentre",
    "prettier",
    "prettier-plugin",
    "typescript"
  ],
  "repository": "https://github.com/gicentre/prettier-plugin-elm",
  "license": "BSD-3-Clause",
  "main": "dist",
  "scripts": {
    "build": "tsc --project .",
    "clean": "rimraf cache coverage dist",
    "fix": "yarn fix:prettier; yarn fix:yarn-deduplicate",
    "fix:prettier": "prettier --write .",
    "fix:yarn-deduplicate": "yarn install && yarn-deduplicate --strategy=fewer && yarn install",
    "lint": "yarn lint:prettier && yarn lint:tsc && yarn lint:tslint && yarn lint:yarn-deduplicate",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --project .",
    "lint:tslint": "tslint --project .",
    "lint:yarn-deduplicate": "yarn-deduplicate --fail --list --strategy=fewer",
    "prepublishOnly": "yarn lint && yarn test",
    "test": "yarn clean && yarn build && jest"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "elm-format": "0.8.3",
    "execa": "^4.0.0",
    "make-dir": "^3.0.2",
    "object-hash": "^2.0.3",
    "serialize-error": "^6.0.0",
    "temp-dir": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/object-hash": "^1.3.1",
    "@types/prettier": "^2.0.0",
    "@types/rimraf": "^3.0.0",
    "jest": "^25.3.0",
    "jest-junit": "^10.0.0",
    "prettier": "^2.0.4",
    "prettier-plugin-packagejson": "^2.2.0",
    "rimraf": "^3.0.2",
    "sleep-promise": "^8.0.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "yarn-deduplicate": "^2.0.0"
  }
}
