{
  "name": "prettier-plugin-elm",
  "version": "0.4.1",
  "license": "BSD-3-Clause",
  "repository": "https://github.com/gicentre/prettier-plugin-elm",
  "description": "Adds Elm language support to Prettier via elm-format",
  "scripts": {
    "clean:coverage": "rimraf coverage",
    "clean:cache": "rimraf cache",
    "lint": "tsc --project . && tslint --project . && prettier --check {*,src/**/*}.{md,js,json,ts}",
    "test": "jest -c jest.test.config.js --no-coverage",
    "test:coverage": "jest -c jest.test.config.js",
    "format": "prettier --write {*,src/**/*}.{md,js,json,ts}",
    "prepublishOnly": "yarn lint && yarn test"
  },
  "main": "src",
  "files": [
    "src"
  ],
  "keywords": [
    "elm",
    "elm-format",
    "gicentre",
    "prettier",
    "prettier-plugin"
  ],
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "prettier": {},
  "dependencies": {
    "elm-format": "^0.8.1",
    "execa": "^1.0.0",
    "make-dir": "^1.3.0",
    "object-hash": "^1.3.0",
    "serialize-error": "^3.0.0",
    "temp-dir": "^1.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "jest": "^23.6.0",
    "prettier": "prettier/prettier#439b1649ff9298b56b4f5e68214de6c2b29c6f26",
    "rimraf": "^2.6.2",
    "sleep-promise": "^8.0.1",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  }
}
